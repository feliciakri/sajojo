# 1 "arch/arm/boot/dts/overlays/i2c1-bcm2708-overlay.dts"
# 1 "<command-line>"
# 1 "arch/arm/boot/dts/overlays/i2c1-bcm2708-overlay.dts"







/dts-v1/;
/plugin/;

/{
   compatible = "brcm,bcm2708";

   fragment@0 {
      target = <&i2c1>;
      __overlay__ {
         pinctrl-0 = <&i2c1_pins>;
         status = "okay";
      };
   };

   fragment@1 {
      target = <&i2c1_pins>;
         pins: __overlay__ {
         brcm,pins = <2 3>;
         brcm,function = <4>;
      };
   };
   __overrides__ {
      sda1_pin = <&pins>,"brcm,pins:0";
      scl1_pin = <&pins>,"brcm,pins:4";
      pin_func = <&pins>,"brcm,function:0";
   };
};
