# 1 "arch/arm/boot/dts/overlays/rpi-tv-overlay.dts"
# 1 "<command-line>"
# 1 "arch/arm/boot/dts/overlays/rpi-tv-overlay.dts"


/dts-v1/;
/plugin/;

/ {
 compatible = "brcm,bcm2708", "brcm,bcm2709";

 fragment@0 {
  target = <&spi0>;
  __overlay__ {

   #address-cells = <1>;
   #size-cells = <0>;

   status = "okay";

   spidev@0 {
    status = "disabled";
   };

   cxd2880@0 {
    compatible = "sony,cxd2880";
    reg = <0>;
    spi-max-frequency = <50000000>;
    status = "okay";
   };
  };
 };

};
